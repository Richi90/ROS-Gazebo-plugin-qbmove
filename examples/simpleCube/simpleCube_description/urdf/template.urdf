<?xml version="1.0"?>
<robot name="your_robot_name">

    <link name="base">
    </link> 
    
    <joint name="first_joint" type="revolute">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="base"/>
        <child link="first_link"/>
        <limit effort="300" velocity="100" lower="-3.14" upper="3.14" />
        <axis xyz="0 0 1"/>
    </joint>

    <link name="first_link">
       <!-- your link yere -->
    </link>


    <!-- QBMOVE ADVANCED PLUGIN -->
    <gazebo>
        <plugin name="advanced_vsa" filename="libadvanced_plugin.so">
            <joint>first_joint</joint>   
        </plugin>
    </gazebo>

    <!-- Similarly other links/joints -->

</robot>
